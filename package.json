{
  "name": "kine-desk",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=4096' nuxt build ",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "pnpm run build && wrangler dev",
    "postinstall": "nuxt prepare",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "db:gen": "drizzle-kit generate",
    "db:mig": "wrangler d1 migrations apply kine",
    "db:mig-remote": "wrangler d1 migrations apply kine --remote",
    "db:mig-prev": "wrangler d1 migrations apply kine --remote --preview",
    "db:seed": "curl -X POST http://localhost:3000/api/db/seed",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "nuxi typecheck"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.985.0",
    "@aws-sdk/s3-request-presigner": "^3.985.0",
    "@internationalized/date": "^3.11.0",
    "@pinia/colada": "^0.21.3",
    "@pinia/colada-nuxt": "0.3.1",
    "@pinia/nuxt": "0.11.3",
    "@unovis/ts": "^1.6.4",
    "@unovis/vue": "^1.6.4",
    "@vueuse/nuxt": "^14.2.0",
    "@vueuse/router": "^14.2.0",
    "better-auth": "^1.4.18",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "drizzle-zod": "^0.8.3",
    "nuxt": "^4.3.1",
    "pinia": "^3.0.4",
    "uuid": "^13.0.0",
    "vue": "^3.5.27",
    "vue-router": "^5.0.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@iconify-json/hugeicons": "^1.2.23",
    "@iconify-json/lucide": "^1.2.89",
    "@iconify-json/simple-icons": "^1.2.70",
    "@nuxt/ui": "^4.4.0",
    "@pinia/colada-devtools": "^0.4.2",
    "@vitest/coverage-v8": "^4.0.18",
    "drizzle-kit": "^0.31.8",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.13.1",
    "prettier": "^3.8.1",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18",
    "wrangler": "^4.63.0"
  },
  "packageManager": "pnpm@10.29.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "maplibre-gl",
      "sharp",
      "vue-demi",
      "workerd"
    ]
  }
}
