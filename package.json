{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "NODE_OPTIONS='--max-old-space-size=4096' nuxt build ",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "pnpm run build && wrangler dev",
    "postinstall": "nuxt prepare",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "db:gen": "drizzle-kit generate",
    "db:mig": "wrangler d1 migrations apply kine",
    "db:mig-remote": "wrangler d1 migrations apply kine --remote",
    "db:mig-prev": "wrangler d1 migrations apply kine --remote --preview"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.940.0",
    "@aws-sdk/s3-request-presigner": "^3.940.0",
    "@internationalized/date": "^3.10.0",
    "@paralleldrive/cuid2": "^2.3.1",
    "@unovis/ts": "^1.6.2",
    "@unovis/vue": "^1.6.2",
    "@vueuse/nuxt": "^14.0.0",
    "better-auth": "^1.4.3",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.7",
    "drizzle-zod": "^0.8.3",
    "nuxt": "^4.2.1",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@iconify-json/lucide": "^1.2.75",
    "@iconify-json/simple-icons": "^1.2.60",
    "@nuxt/ui": "^4.2.1",
    "@types/node": "^24.10.1",
    "drizzle-kit": "^0.31.7",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.12.9",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.1",
    "wrangler": "^4.51.0"
  },
  "packageManager": "pnpm@10.23.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "maplibre-gl",
      "sharp",
      "vue-demi",
      "workerd"
    ]
  }
}