{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "pnpm run build && wrangler dev",
    "postinstall": "nuxt prepare",
    "deploy": "pnpm run build && wrangler deploy",
    "cf-typegen": "wrangler types",
    "db:gen": "drizzle-kit generate",
    "db:mig": "wrangler d1 migrations apply kine",
    "db:mig-remote": "wrangler d1 migrations apply kine --remote",
    "db:mig-prev": "wrangler d1 migrations apply kine --remote --preview"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "better-auth": "^1.3.27",
    "drizzle-orm": "^0.44.6",
    "nuxt": "^4.1.3",
    "vue": "^3.5.22",
    "vue-router": "^4.6.0"
  },
  "devDependencies": {
    "@nuxt/ui": "^4.0.1",
    "drizzle-kit": "^0.31.5",
    "nitro-cloudflare-dev": "^0.2.2",
    "nitropack": "^2.12.7",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.7.0",
    "wrangler": "^4.43.0"
  },
  "packageManager": "pnpm@10.18.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "vue-demi",
      "workerd"
    ]
  }
}